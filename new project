            background-color: #f8f9fa;
        }

        h1 {
            color: #343a40;
        }

        #articles, #results {
            display: flex;
            flex-wrap: wrap;
            gap: 1rem;
        }
        .font-colo{
            font-size:35px;
            color:red;
        }

